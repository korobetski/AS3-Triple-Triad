		public static function getCardsByRarities(rarities:Array, collection:String = "ff14_"):Array {
			var _cards:Array = [];
			var listLength:uint = cards[collection.toUpperCase() + 'DATAS'].length;
			for (var i:uint = 0; i < listLength; i++ ) {
				var c:Object = cards[collection.toUpperCase() + 'DATAS'][i];
				if (ArrayUtil.arrayContainsValue(rarities, c.rarity)) {
					_cards.push(i);
				}
			}
			return _cards;
		}
